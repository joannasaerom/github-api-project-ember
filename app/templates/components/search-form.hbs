<form {{action 'lookUp' on='submit'}}>
  <div class="form-group">
    <label for="location">Location</label>
    {{input value=location type="text" required="required"}}
  </div>
  <div class="form-group">
    <label for="language">Language</label>
    {{input value=language type="text" required="required"}}
  </div>
  <button class="btn">Search</button>
</form>
