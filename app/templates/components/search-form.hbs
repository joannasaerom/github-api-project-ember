<div class="search-form">
  <form class="form-inline" {{action 'lookUp' on='submit'}}>
    <div class="form-group">
      <label for="location">Location</label>
      {{input value=location type="text" class="form-control" required="required"}}
    </div>
    <div class="form-group">
      <label for="language">Language</label>
      {{input value=language type="text" class="form-control" required="required"}}
    </div>
    <button class="btn btn-primary">Search</button>
  </form>
</div>
