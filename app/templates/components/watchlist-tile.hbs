<div class="row">
  {{#each watchUser.users as |developer|}}
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title title">{{developer.name}}</h3>
        </div>
        <div class="panel-body">
          <p>
            Username: {{developer.login}}
          </p>
          <p>
            Bio: {{developer.bio}}
          </p>
          <p>
            Email: {{developer.email}}
          </p>
        </div>
        <div class="panel-footer"><span class="clickable" {{action 'removeFromWatch' developer}}>remove from watchlist</span> / send email </div>
      </div>
    </div>

  {{/each}}

</div>
